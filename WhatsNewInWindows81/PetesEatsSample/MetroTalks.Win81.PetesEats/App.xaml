<Application
    x:Class="MetroTalks.Win81.PetesEats.App"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:MetroTalks.Win81.PetesEats"
    xmlns:localData="using:MetroTalks.Win81.PetesEats.Data"
    RequestedTheme="Dark">

  <Application.Resources>
    <!-- Application-specific resources -->
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="/Resources/Colours.xaml"/>
            </ResourceDictionary.MergedDictionaries>
            
            <x:String x:Key="AppName">Pete's Eats</x:String>
            
            
        </ResourceDictionary>
  </Application.Resources>
</Application>
